{
  "name": "wifi-usage-statistics",
  "version": "1.0.1",
  "description": "app to monitor number of logged in wifi users",
  "main": "server.jsx",
  "repository": "https://github.com/taras-bohdan/wifi_stats",
  "engines": {
    "node": "8.9.1"
  },
  "scripts": {
    "test": "jest",
    "test-watch": "jest --watch",
    "start": "node dist/server.js",
    "postinstall": "npm run build:prod",
    "build": "webpack --env.dev --config ./webpack/webpack.config.babel.js",
    "build:client": "webpack --env.dev --config ./webpack/webpack.client.config.babel.js",
    "build:server": "webpack --env.dev --config ./webpack/webpack.server.config.babel.js",
    "build:prod": "webpack --env.prod --config ./webpack/webpack.config.babel.js",
    "build:prod-client": "webpack --env.prod --config ./webpack/webpack.client.config.babel.js",
    "build:prod-server": "webpack --env.prod --config ./webpack/webpack.server.config.babel.js",
    "watch": "webpack --env.dev --watch --config ./webpack/webpack.config.babel.js",
    "watch:client": "webpack --env.dev --watch --config ./webpack/webpack.client.config.babel.js",
    "watch:server": "webpack --env.dev --watch --config ./webpack/webpack.server.config.babel.js"
  },
  "author": "",
  "license": "ISC",
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "html"
    ]
  },
